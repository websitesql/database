{
	"name": "websitesql/database",
	"description": "Database component for WebsiteSQL framework",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Alan Tiller",
			"email": "alan@alantiller.com"
		}
	],
	"require": {
		"php": "^8.0",
		"catfan/medoo": "^2.1",
		"psr/container": "^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"WebsiteSQL\\Database\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WebsiteSQL\\Database\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"test": "phpunit"
	}
}